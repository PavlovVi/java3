import java.util.ArrayList;

public class Main {
    public static void main(String[] args) {
        ArrayList<Integer> numbers = new ArrayList<Integer>();
        numbers.add(1);
        numbers.add(2);
        numbers.add(3);
        numbers.add(4);
        numbers.add(5);
        numbers.add(6);
        numbers.add(7);
        numbers.add(8);
        numbers.add(9);
        numbers.add(10);
        // Создаем пустой список, куда будем добавлять нечётные числа
        ArrayList<Integer> result = new ArrayList<Integer>();
        for (int number : numbers) {
            if (number % 2 == 1) {
                result.add(number);
            }
        }
        numbers = result;
        System.out.println(numbers);
    }
}
